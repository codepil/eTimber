<script language="javascript">
function resultPoll()
{
	url = "http://www.indiavarta.com/Poll/Index.asp"
	resWin = window.open(url,'win','');
	resWin.focus()
}
function pollOpen(cat)
{
	len = 3
	for(i = 0; i < len; i++)
	{
		if(document.oPoll.R1[i].checked)
			val = document.oPoll.R1[i].value;
	}
	url = "http://www.indiavarta.com/Poll/Index.asp?R1="+val
	pollWin = window.open(url,'win','');
	pollWin.focus()
}
</script>

