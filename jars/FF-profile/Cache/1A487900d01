<script language="javascript" type="text/javascript">
<!--
function loadLang() {

	var url = getURL();
	self.location.href = url;
}

// this function can only be used by the language drop-down
function getURL() {

	selectBox = document.getElementById('langSelect');
	destLang = selectBox.options[selectBox.selectedIndex].value;

	var url = self.location.href;
	// redirIndex is the base url for the host
	var redirIndex;
	if (url.indexOf('https') > -1) {
		redirIndex = 'https://' + document.getElementById('shost').value + '/';
	} else {
		redirIndex = 'http://' + document.getElementById('host').value + '/';
	}
	
	var deltron = url.indexOf('?');
	if (deltron > 0) {
		// urlsplit gives us an array with the pieces of the url separated by ?'s
		var urlsplit = url.split('?');
		// base_url is everything before the first ?
		base_url = urlsplit[0];
	
		if (redirIndex == base_url) {
			// if base url is equal to the host, then we are on the home page
			base_url = base_url + 'index.jsp?language=' + destLang;
			return base_url;
		}

		var ampcount = urlsplit[1].split('&').length - 1;
		var ampsplit = urlsplit[1].split('&');
		var first_param = true;
		
		for (var i=0; i<=ampcount; i++) {
			if (ampsplit[i].indexOf('language') < 0 && true == first_param ) {
				base_url = base_url + '?' + ampsplit[i];
				first_param = false;
			} else if (ampsplit[i].indexOf('language') < 0 && false == first_param) {
				base_url = base_url + '&' + ampsplit[i];
			}
		}
		
		if (true == first_param) {
			base_url = base_url + "?language=" + destLang;
		} else {
			base_url = base_url + "&language=" + destLang;
 		}
		return base_url;
	} else {
		if (redirIndex == url) {
			// if base url is equal to the host, then we are on the home page
			url = url + 'index.jsp?language=' + destLang;
			return url;
		} else {
			url = url + '?language=' + destLang;	
			return url;
		}
	}
}

//-->
</script>


